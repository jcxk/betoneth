{
  "name": "bettingon",
  "version": "0.0.1",
  "description": "Betting on ethers",
  "scripts": {
    "lint": "eslint ./",
    "build": "webpack",
    "build-dapp": "NODE_ENV=production webpack --config dapp/webpack.prod.config.js",
    "dev": "webpack-dev-server",
    "dev-dapp": "webpack-dev-server --debug --hot --open --port=8081 --config dapp/webpack.config.js --content-base dapp/build",
    "deploy-dapp": "surge dapp/build bettingon.surge.sh",
    "dapp-serve": "npm run build-dapp && http-server-spa dapp/build",
    "testrpc": "testrpc -d -u 0 -u 1",
    "contract-update": "truffle migrate --reset",
    "dev-dapp-rpc": "npm-run-all --parallel dev-dapp contract-update testrpc",
    "eslint-dapp": "eslint -c dapp/.eslintrc.json dapp"
  },
  "author": [
    "Juan Carlos Escamilla",
    "Blanca Baron",
    "Adria Massanet <adria@codecontext.io>"
  ],
  "license": "GPL-3.0",
  "dependencies": {
    "ethpm": "0.0.14",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.17.4",
    "moment": "^2.18.1",
    "moment-duration-format": "^1.3.0",
    "npm-run-all": "^4.0.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-scripts": "^0.9.5",
    "react-table": "^6.5.1",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.7.2",
    "redux-form": "^6.8.0",
    "redux-form-material-ui": "^4.2.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.0.16",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.4",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^2.3.1",
    "ethereumjs-testrpc": "^4.0.1",
    "html-webpack-plugin": "^2.29.0",
    "http-server-spa": "^1.2.0",
    "json-loader": "^0.5.4",
    "react-hot-loader": "^3.0.0-beta.7",
    "style-loader": "^0.13.2",
    "surge": "^0.19.0",
    "truffle": "^3.4.5",
    "truffle-contract": "^1.1.11",
